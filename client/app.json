{
  "expo": {
    "name": "Auction App TVP",
    "slug": "auctionlocalbid",
    "version": "14.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#FFFFFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.imaker.auctionapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "splash": {
        "mdpi": "assets/splash.png",
        "hdpi": "assets/splash/hdpi.png",
        "xhdpi": "assets/splash/xhdpi.png",
        "xxhdpi": "assets/splash/xxhdpi.png",
        "xxxhdpi": "assets/splash/xxxhdpi.png"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "",
    "githubUrl": "https://github.com/MisterGalactic/bid-local"
  }
}
